{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "Estudando a criação de api em nodejs",
  "main": "index.js",
  "scripts": {
    "test": "jest --watch --silent --colors --noStackTrace",
    "test:staged": "jest --passWithNoTests --findRelatedTests --silent --noStackTrace",
    "test:ci": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "npm run test:staged",
      "git add"
    ]
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rickyalbuq/clean-node-api.git"
  },
  "keywords": [
    "api",
    "nodejs",
    "tdd",
    "cleanCode"
  ],
  "author": "Henrique Albuquerque",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rickyalbuq/clean-node-api/issues"
  },
  "homepage": "https://github.com/rickyalbuq/clean-node-api#readme",
  "devDependencies": {
    "husky": "^9.1.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "standard": "^17.1.0"
  }
}